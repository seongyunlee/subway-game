{
  "family": "ZHC-API-SPRING",
  "executionRoleArn": "arn:aws:iam::590184107250:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "zeehacheol",
      "image": "590184107250.dkr.ecr.ap-northeast-2.amazonaws.com/zeehacheol:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}